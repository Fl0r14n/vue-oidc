{
  "name": "vue-oidc",
  "version": "2.0.2",
  "author": "Fl0r14n <florian.chis@gmail.com>",
  "homepage": "https://github.com/Fl0r14n/vue-oidc",
  "description": "A fully OAuth2.1 compliant angular library",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "oauth",
    "oauth2",
    "oidc",
    "pkce",
    "authorization",
