      "types": "./dist/index.d.ts",
      "import": "./dist/vue-oidc.js",
      "require": "./dist/vue-oidc.umd.cjs"
    },
    "./dist/vue-oidc.css": "./dist/vue-oidc.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test:unit": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "axios": "^1.8.4"
  },
  "peerDependencies": {
    "vue": ">3.5.0",
    "vue-i18n": ">10.0.0",
    "vuetify": ">3.0.0"
  }
}
