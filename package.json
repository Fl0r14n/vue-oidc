{
  "name": "vue-oidc-sample",
  "version": "1.0.6",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prod": "vite --mode production",
    "build-only": "vite build",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:lib": "cd lib && npm run build",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:ssr": "npm run build:client && npm run build:server",
    "ssr:dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=development PORT=4200 node server",
    "ssr:prod": "NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=production PORT=4200 node server",
    "preview": "vite preview",
    "test:unit": "vitest",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "pinia": "^2.1.7",
    "vue": "^3.4.27",
    "vue-i18n": "^9.13.1",
    "vue-oidc": "file:lib",
    "vue-router": "^4.3.2",
    "vuetify": "^3.6.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.11",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "compression": "^1.7.4",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "express": "^4.19.2",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "resolve-tspaths": "^0.8.18",
    "sass": "^1.77.1",
    "sirv": "^2.0.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.17"
  }
}
