{
  "name": "vue-oidc-sample",
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prod": "vite --mode production",
    "build": "cd lib && npm run build",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:ssr": "run-p build:client build:server",
    "ssr:dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=development PORT=4200 node server",
    "ssr:prod": "NODE_ENV=production PORT=4200 node server",
    "test:unit": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "publish": "npm run build && cd lib && npm publish"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.2",
    "vue-oidc": "file:lib",
    "vue-router": "^4.5.0",
    "vuetify": "^3.7.15"
  },
  "devDependencies": {
    "globals": "^16.0.0",
    "@eslint/js": "^9.22.0",
    "@tsconfig/node23": "^23.0.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "compression": "^1.8.0",
    "eslint-plugin-vue": "^10.0.0",
    "express": "^4.21.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.85.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.1",
    "vite-plugin-vuetify": "^2.1.0",
    "vitest": "^3.0.8",
    "vite-plugin-dts": "^4.5.3",
    "serve-static": "^1.16.2"
  }
}
