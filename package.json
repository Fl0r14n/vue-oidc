{
  "name": "vue-oidc-sample",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prod": "vite --mode production",
    "build": "cd lib && npm run build",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:ssr": "run-p build:client build:server",
    "ssr:dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=development PORT=3000 node server",
    "ssr:prod": "NODE_ENV=production PORT=3000 node server",
    "test:unit": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "publish": "npm run build && cd lib && npm publish"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "pinia": "^3.0.2",
    "vue": "^3.5.13",
    "vue-oidc": "file:lib",
    "vue-router": "^4.5.0",
    "vuetify": "^3.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@tsconfig/node23": "^23.0.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/eslint-plugin": "^1.1.40",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "compression": "^1.8.0",
    "eslint-plugin-vue": "^10.0.0",
    "express": "^5.1.0",
    "globals": "^16.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.86.3",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-plugin-vuetify": "^2.1.1",
    "vitest": "^3.1.1",
    "vite-plugin-dts": "^4.5.3",
    "serve-static": "^2.2.0"
  }
}
