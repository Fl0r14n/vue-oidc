{
  "name": "vue-oidc-sample",
  "version": "2.0.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prod": "vite --mode production",
    "ssr:dev": "bun run index",
    "ssr:prod": "NODE_ENV=production bun run index",
    "build": "run-p type-check build:lib \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "build:lib": "cd lib && run-s build",
    "build:ssr:client": "vite build --ssrManifest --outDir dist/client",
    "build:ssr:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:ssr": "run-p build:lib build:ssr:*",
    "publish": "run-s build && cd lib && npm publish"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-oidc": "file:lib",
    "vue-router": "^4.6.3",
    "vuetify": "^3.10.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@microsoft/api-extractor": "^7.53.3",
    "@tsconfig/node24": "^24.0.1",
    "@types/bun": "^1.3.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.9.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.4.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/language-core": "^3.1.2",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.2",
    "serve-static": "^2.2.0",
    "typescript": "^5.9.3",
    "unplugin-dts": "^1.0.0-beta.6",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.2"
  }
}
