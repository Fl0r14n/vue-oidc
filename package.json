{
  "name": "vue-oidc-sample",
  "version": "2.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prod": "vite --mode production",
    "build": "cd lib && npm run build",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:ssr": "run-p build:client build:server",
    "ssr:dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=development PORT=3000 node server",
    "ssr:prod": "NODE_ENV=production PORT=3000 node server",
    "test:unit": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "publish": "npm run build && cd lib && npm publish"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-oidc": "file:lib",
    "vue-router": "^4.5.1",
    "vuetify": "^3.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tsconfig/node23": "^23.0.2",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "compression": "^1.8.1",
    "eslint-plugin-vue": "^10.3.0",
    "express": "^5.1.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.89.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-vuetify": "^2.1.1",
    "vitest": "^3.2.4",
    "vite-plugin-dts": "^4.5.4",
    "serve-static": "^2.2.0"
  }
}
