{
  "name": "vue-oidc-sample",
  "version": "1.0.7",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prod": "vite --mode production",
    "build": "cd lib && npm run build",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:ssr": "npm run build:client && npm run build:server",
    "ssr:dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=development PORT=4200 node server",
    "ssr:prod": "NODE_ENV=production PORT=4200 node server",
    "test:unit": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "pinia": "^2.2.3",
    "vue": "^3.5.10",
    "vue-i18n": "^9.14.1",
    "vue-oidc": "file:lib",
    "vue-router": "^4.4.5",
    "vuetify": "^3.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@tsconfig/node22": "^22.0.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.7.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.0.0-rc.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "compression": "^1.7.4",
    "eslint-plugin-vue": "^9.28.0",
    "express": "^4.21.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "resolve-tspaths": "^0.8.22",
    "sass-embedded": "^1.79.4",
    "sirv": "^2.0.4",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "^2.1.1"
  }
}
